<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainPackage.InitialController">
  <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
    <children>
      <ComboBox fx:id="portList" layoutX="184.0" layoutY="149.0" prefHeight="26.0" prefWidth="105.0" />
      <Text layoutX="158.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Connect to the Arduino and Server before proceeding." />
      <Button fx:id="connectButton" layoutX="341.0" layoutY="149.0" mnemonicParsing="false" onAction="#connect_to_arduino" prefHeight="26.0" prefWidth="111.0" text="Connect" />
      <Button fx:id="serverButton" layoutX="257.0" layoutY="265.0" mnemonicParsing="false" onAction="#connect_to_server" text="Connect to Server" />
         <Button fx:id="refreshButton" layoutX="207.0" layoutY="187.0" mnemonicParsing="false" onAction="#refreshCOMports" prefHeight="26.0" prefWidth="58.0" text="Refresh" />
    </children>
  </AnchorPane>
</VBox>
